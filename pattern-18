#include <bits/stdc++.h>
using namespace std;
int main() {
    int n;
    char ch;
    cout<<"Enter the number of rows:"<<endl;
    cin>>n;
    for(int i=n;i>0;i--){
        for(int j=n;j>=i;j--){
            if(j==n)
            ch=64+n-(n-i);
            cout<<ch<<" ";
            ch++;
        }
        cout<<endl;
    }
    return 0;
}
